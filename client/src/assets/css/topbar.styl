.topbar
  margin-top 20px
  overflow auto
  text-align center
  li
    display inline-block
    position relative
    margin 0 20px
    color violet
    a
      color violet
    i
      opacity .4
      position absolute
      line-height 23px
      left -20px
      transition all .1s ease-in
    &:hover
      color topmenu
      i
        opacity 1
        // left -20px